{
    "manifest_version": 2,
    "name": "SMI 재생기",
    "version": "0.1",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "upload.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}